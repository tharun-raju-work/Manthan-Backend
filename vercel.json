{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SSL_ENABLED": "false",
    "REDIS_ENABLED": "false",
    "ENABLE_CLUSTERING": "false",
    "POSTGRES_SSL": "true",
    "PORT": "3000"
  }
} 